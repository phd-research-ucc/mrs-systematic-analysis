	keywords = {OR in health services; Operating room; Intensive care unit; Chance-constraint optimisation model; Discrete-event simulation},
	number = {1},
	pages = {243--260},
	title = {Tactical blueprints for surgical weeks --An integrated approach for operating rooms and intensive care units},
	url = {https://www.sciencedirect.com/science/article/pii/S0377221721005117},
	volume = {298},
	year = {2022},
