	issn = {2474-9842},
	journal = {BJS Open},
	month = {11},
	number = {6},
	pages = {zrad113},
	title = {{Machine learning models to predict surgical case duration compared to current industry standards: scoping review}},
	url = {https://doi.org/10.1093/bjsopen/zrad113},
	volume = {7},
	year = {2023},
	bdsk-url-1 = {https://doi.org/10.1093/bjsopen/zrad113}}

