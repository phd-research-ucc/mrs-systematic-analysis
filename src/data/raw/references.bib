	journal = {Stochastic Environmental Research and Risk Assessment},
	number = {9},
	pages = {2365--2377},
	title = {Economic-environmental trade-offs and the conservativeness of the upper partial moment},
	url = {https://doi.org/10.1007/s00477-016-1371-y},
	volume = {31},
	year = {2017},
	bdsk-url-1 = {https://doi.org/10.1007/s00477-016-1371-y}}

