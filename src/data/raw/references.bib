	isbn = {0305-0548},
	journal = {Computers \& Operations Research},
	keywords = {First-order linear programming; Heuristic; Column generation; Nurse rostering},
	pages = {104945},
	title = {First-order linear programming in a column generation-based heuristic approach to the nurse rostering problem},
	url = {https://www.sciencedirect.com/science/article/pii/S0305054820300629},
	volume = {120},
