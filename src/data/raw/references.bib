	journal1 = {Scheduling in Healthcare and Industrial Systems},
	keywords = {Scheduling; Health care management; Mixed-integer programming; Genetic algorithm},
	number = {2},
	pages = {212--220},
	title = {Scheduling an operating theatre under human resource constraints},
	url = {https://www.sciencedirect.com/science/article/pii/S0360835209000084},
	volume = {58},
	year = {2010},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0360835209000084},
