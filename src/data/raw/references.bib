	journal1 = {IISE Transactions},
	journal2 = {IISE Transactions},
	month = {02},
	number = {2},
	pages = {216--235},
	publisher = {Taylor \& Francis},
	title = {Operating room scheduling problem under uncertainty: Application of continuous phase-type distributions},
	type = {doi: 10.1080/24725854.2019.1628372},
	url = {https://doi.org/10.1080/24725854.2019.1628372},
	volume = {52},
	year = {2020},
