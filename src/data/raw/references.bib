	doi = {10.1080/24725854.2019.1628372},
	isbn = {2472-5854},
	journal = {IISE Transactions},
	journal1 = {IISE Transactions},
	journal2 = {IISE Transactions},
	month = {02},
	number = {2},
	pages = {216--235},
	publisher = {Taylor \& Francis},
	title = {Operating room scheduling problem under uncertainty: Application of continuous phase-type distributions},
	type = {doi: 10.1080/24725854.2019.1628372},
	url = {https://doi.org/10.1080/24725854.2019.1628372},
	volume = {52},
	year = {2020},
	year1 = {2020},
	bdsk-url-1 = {https://doi.org/10.1080/24725854.2019.1628372}}

@article{x272,
	abstract = {This paper studies how to schedule medical appointments with time-dependent patient no-show behavior and random service times. The problem is motivated by our studies of independent datasets from countries in two continents that unanimously identify a significant time-of-day effect on patient show-up probabilities. We deploy a distributionally robust model, which minimizes the worst-case total expected costs of patient waiting and service provider?s idling and overtime, by optimizing the scheduled arrival times of patients. This model is challenging because evaluating the total cost for a given schedule involves a linear program with uncertainties present in both the objective function and the right-hand side of the constraints. In addition, the ambiguity set considered contains discrete uncertainties and complementary functional relationships among these uncertainties (namely, patient no-shows and service durations). We show that when patient no-shows are exogenous (i.e., time-independent), the problem can be reformulated as a copositive program and then be approximated by semidefinite programs. When patient no-shows are endogenous on time (and hence on the schedule), the problem becomes a bilinear copositive program. We construct a set of dual prices to guide the search for a good schedule and use the technique iteratively to obtain a near-optimal solution. Our computational studies reveal a significant reduction in total expected cost by taking into account the time-of-day variation in patient show-up probabilities as opposed to ignoring it.This paper was accepted by David Simchi-Levi, optimization.},
	annote = {doi: 10.1287/mnsc.2019.3366},
	author = {Kong, Qingxia and Li, Shan and Liu, Nan and Teo, Chung-Piaw and Yan, Zhenzhen},
	date = {2020/08/01},
	date-added = {2023-11-17 21:28:28 +0200},
	date-modified = {2023-11-17 21:28:28 +0200},
	doi = {10.1287/mnsc.2019.3366},
	isbn = {0025-1909},
	journal = {Management Science},
	journal1 = {Management Science},
	journal2 = {Management Science},
	month = {2023/11/17},
	n2 = {This paper studies how to schedule medical appointments with time-dependent patient no-show behavior and random service times. The problem is motivated by our studies of independent datasets from countries in two continents that unanimously identify a significant time-of-day effect on patient show-up probabilities. We deploy a distributionally robust model, which minimizes the worst-case total expected costs of patient waiting and service provider?s idling and overtime, by optimizing the scheduled arrival times of patients. This model is challenging because evaluating the total cost for a given schedule involves a linear program with uncertainties present in both the objective function and the right-hand side of the constraints. In addition, the ambiguity set considered contains discrete uncertainties and complementary functional relationships among these uncertainties (namely, patient no-shows and service durations). We show that when patient no-shows are exogenous (i.e., time-independent), the problem can be reformulated as a copositive program and then be approximated by semidefinite programs. When patient no-shows are endogenous on time (and hence on the schedule), the problem becomes a bilinear copositive program. We construct a set of dual prices to guide the search for a good schedule and use the technique iteratively to obtain a near-optimal solution. Our computational studies reveal a significant reduction in total expected cost by taking into account the time-of-day variation in patient show-up probabilities as opposed to ignoring it.This paper was accepted by David Simchi-Levi, optimization.},
	number = {8},
	pages = {3480--3500},
	publisher = {INFORMS},
	title = {Appointment Scheduling Under Time-Dependent Patient No-Show Behavior},
	type = {doi: 10.1287/mnsc.2019.3366},
