	keywords = {logic programming methodology and applications; knowledge representation and nonmonotonic reasoning},
	number = {2},
	pages = {229-253},
	publisher = {Cambridge University Press},
	title = {Operating Room (Re)Scheduling with Bed Management via ASP},
	url = {https://www.cambridge.org/core/article/operating-room-rescheduling-with-bed-management-via-asp/2E7EC6B0FD0A0752BD01509067E4598E},
