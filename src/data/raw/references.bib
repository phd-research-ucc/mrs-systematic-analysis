	doi = {https://doi.org/10.1016/j.omega.2019.04.005},
	isbn = {0305-0483},
	journal = {Omega},
	keywords = {Appointment and real-time scheduling; Walk-in patients; Waiting time target; Stochastic programming; Markov decision process},
	pages = {102062},
	title = {Managing appointments with waiting time targets and random walk-ins},
	url = {https://www.sciencedirect.com/science/article/pii/S0305048318307126},
	volume = {95},
	year = {2020},
	bdsk-url-1 = {https://www.sciencedirect.com/science/article/pii/S0305048318307126},
