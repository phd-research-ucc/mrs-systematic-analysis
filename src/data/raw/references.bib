	id = {10.1007/978-3-030-85672-4{\_}27},
	isbn = {978-3-030-85672-4},
	pages = {361--373},
	publisher = {Springer International Publishing},
	title = {Incorporating User Preferences in Multi-objective Feature Selection in Software Product Lines Using Multi-Criteria Decision Analysis},
	year = {2021}}

