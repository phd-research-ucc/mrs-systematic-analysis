@phdthesis{x354,
	abstract = {The continuing growth of the older population has led to a significant increase in demand for rehabilitation over the past two decades and subsequently in long delays in admission to rehabilitation care. These delays have two important implications: Firstly, delayed patients continue occupying their beds in acute care, blocking new patients from being admitted and incurring additional costs of operating acute beds. Secondly, admission delays could adversely impact rehabilitation outcomes. In this thesis, we propose new empirical and stochastic models of patient flow and use them to develop insights and operational interventions aimed at reducing admission delays. In Chapter 2, we investigate the sources of admission delays in Low Tolerance Long Duration (LTLD) rehabilitation. Admission delays can be caused by capacity constraints or extra processing requirements to plan rehabilitation activities. However, standard data only includes a single (combined) measure of delay, and the bed allocation decisions in practice can be quite complicated with multiple determinant factors. To address these challenges, we propose a Hidden Markov Model to estimate the status-quo bed allocation policy with unobserved processing times. The estimation results are then combined with a high-fidelity queueing model of the system to evaluate the effectiveness of various interventions to reduce admission delays. In Chapter 3, we focus on High Tolerance Short Duration (HTSD) rehabilitation and investigate the causal impact of delays on rehabilitation length-of-stays (LOSs) and outcomes. By utilizing an instrumental variable approach, we find that admission delays increase the rehabilitation LOS and negatively impact the functional outcomes of the patients in rehabilitation. Finally, motivated by the empirical findings of Chapter 3, in Chapter 4, we develop approximately optimal scheduling policies for multiclass queues with wait-dependent service times. We propose a simulation-based approximate policy iteration algorithm, wherein the policy improvement step is guided through Machine Learning-based classifiers and the policy evaluation is accelerated using a coupling approach. Using extensive numerical experiments, we investigate the scalability of the proposed algorithm and further apply it in a Case Study calibrated using HTSD rehabilitation data.},
	accepted = {2023-11},
	advisor = {Sarhangian, Vahid},
	author = {Gorgulu, Berk},
	classification = {0546},
	convocation = {2023-11},
	degree = {Ph.D.},
	department = {Mechanical and Industrial Engineering},
	keywords = {approximate dynamic programming, causal inference, healthcare operations, hidden Markov models, patient flow, structural estimation},
	month = {11},
	school = {University of Toronto},
	title = {Stochastic and Empirical Models in Support of Managing Patient Flow from Acute to Rehabilitation Care},
	url = {http://hdl.handle.net/1807/130486},
	year = {2023},
	bdsk-url-1 = {http://hdl.handle.net/1807/130486}}

@mastersthesis{x355,
	abstract = {Total knee and hip arthroplasty (TKA and THA, respectively) are common and resource-intensive procedures contributing to a significant burden on healthcare systems. The overall aim of this thesis project is to identify and develop strategies to improve elective surgical scheduling for TKA and THA. Machine learning and optimization to predict resource utilization-related outcomes such as the duration of surgery and length of stay for arthroplasty patients were identified. A predict-then-optimize approach utilizing neural network models and linear programming was compared to historic scheduling strategies. Data sources for this work include the administrative National Surgical Quality Improvement Program and an institutional database from the Holland Centre at Sunnybrook Health Sciences Centre. The most important features of the neural network models for outcome prediction were identified and compared between databases. Together, these findings are the foundation upon which to develop a ``smart'' surgical scheduling system and improve the efficiency of operating room utilization.},
	accepted = {2023-11},
	advisor = {Whyne, Cari M and Ravi, Bheeshma},
	author = {Lex, Johnathan Robert},
	classification = {0796},
	convocation = {2023-11},
	degree = {M.A.S.},
	department = {Biomedical Engineering},
	keywords = {Arthroplasty, Efficiency, Machine Learning, Optimization, Scheduling},
	month = {11},
	school = {University of Toronto},
	title = {Machine Learning to Predict and Optimize Lower Extremity Arthroplasty Resource Utilization},
	url = {http://hdl.handle.net/1807/130503},
	year = {2023},
	bdsk-url-1 = {http://hdl.handle.net/1807/130503}}

