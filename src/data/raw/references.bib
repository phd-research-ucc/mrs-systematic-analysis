	doi = {https://doi.org/10.1016/j.swevo.2023.101421},
	isbn = {2210-6502},
	journal = {Swarm and Evolutionary Computation},
	keywords = {Flexible job shop scheduling problem; Improved memetic algorithm; Discrete operation sequence flexibility; Flexible sequencing method; Green production},
	pages = {101421},
	title = {Energy-Efficient Flexible Job Shop Scheduling Problem Considering Discrete Operation Sequence Flexibility},
	url = {https://www.sciencedirect.com/science/article/pii/S2210650223001943},
	year = {2023},
