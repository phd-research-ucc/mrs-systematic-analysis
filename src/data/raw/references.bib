	keywords = {Metaheuristic, Optimization, Optimization problem, Population-based, Teaching--learning-based optimization algorithm},
	pages = {126898},
	title = {Advances in teaching--learning-based optimization algorithm: A comprehensive survey(ICIC2022)},
	url = {https://www.sciencedirect.com/science/article/pii/S0925231223010214},
	volume = {561},
