	keywords = {Multi-agent system; Decentralized systems; Learning algorithm; Industry 4.0; Smart manufacturing},
	pages = {338--360},
	title = {Decentralized learning multi-agent system for online machine shop scheduling problem},
	url = {https://www.sciencedirect.com/science/article/pii/S0278612523000286},
	volume = {67},
