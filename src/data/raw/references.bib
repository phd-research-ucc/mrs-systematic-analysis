	isbn = {0304-405X},
	journal = {Journal of Financial Economics},
	number = {1},
	pages = {95--121},
	title = {Optimal rules for ordering uncertain prospects},
	url = {https://www.sciencedirect.com/science/article/pii/0304405X75900252},
