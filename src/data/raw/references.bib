	isbn = {0305-0548},
	journal = {Computers \& Operations Research},
	keywords = {Surgery scheduling problem; Patient priority; Stochastic dynamic programming},
	number = {6},
	pages = {1091--1099},
	title = {An elective surgery scheduling problem considering patient priority},
