	journal = {Applied Soft Computing},
	keywords = {Nurse rostering problem; Decision tree; Greedy search algorithm; Bat algorithm; Particle swarm optimization},
	pages = {106336},
	title = {Developing two heuristic algorithms with metaheuristic algorithms to improve solutions of optimization problems with soft and hard constraints: An application to nurse rostering problems},
	url = {https://www.sciencedirect.com/science/article/pii/S1568494620302763},
