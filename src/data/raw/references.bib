	keywords = {Operating theatre planning and scheduling problem; Open scheduling; Column generation; Heuristic procedure; Hybrid genetic algorithm},
	number = {2},
	pages = {221--230},
	title = {A planning and scheduling problem for an operating theatre using an open scheduling strategy},
